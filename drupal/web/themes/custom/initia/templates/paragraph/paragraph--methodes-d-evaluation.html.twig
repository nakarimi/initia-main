{# Template for the "Méthodes d'évaluation" paragraph type #}


{% if paragraph.field_titre_methodes_d_evaluatio.value %}
	<p class="h2 pt-2 pb-3">{{ paragraph.field_titre_methodes_d_evaluatio.value|raw }}</p>
{% endif %}

<ul>
	{% for item in paragraph.field_liste_methodes %}
		<li>
			{{ item.value }}
		</li>
	{% endfor %}
</ul>

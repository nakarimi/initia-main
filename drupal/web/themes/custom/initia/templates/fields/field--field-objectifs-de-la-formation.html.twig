{# Check if the field has value #}
{% if items|length %}
	{% for item in items %}

		{# Accessing the paragraph referenced in this field #}
		{% set paragraph = item.content['#paragraph'] %}

		{# Access the subfields inside the paragraph: field_liste and field_titre_de_l_objectif #}
		{% set titre_objectif = paragraph.field_titre_de_l_objectif.value %}
		<h4 class="text-blue mb-3" id="Objectifs-de-la-Formation">{{ titre_objectif }}</h4>
		<div class="objectf-block">
			<ul>
				{% for item in paragraph.field_liste %}
					<li>
						<span>
							<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewbox="0 0 24 24" fill="none">
								<path d="M5 12.9998L8.52642 15.8209C9.35374 16.4827 10.5536 16.3846 11.2624 15.5971L19 6.99976" stroke="#96AE41" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>
							</svg>
						</span>
						<p>{{ item.value }}</p>
					</li>
				{% endfor %}
			</ul>
		</div>


		<br>
	{% endfor %}
{% endif %}

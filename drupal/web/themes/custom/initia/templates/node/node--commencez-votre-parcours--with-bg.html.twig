<section id="start-parcours" class="my-5">
<div class="container-fluid">
    <div class="remboursement" style="background-image:url('{{ file_url(node.field_image.entity.uri.value) }}')">
        <div class="row align-items-center">
            <div class="col-md-5">
                <h5>Remboursement des Frais</h5>
				<h2>{{ node.title.value }}</h2>
				{% if node.field_commence_description.value %}
					<p>{{ node.field_commence_description.value|raw }}</p>
				{% endif %}
			</div>
            <div class="col-md-3 offset-md-4">
				<a href="{{ node.field_decouvrir_les_cours.uri }}" target="{{ node.field_decouvrir_les_cours.options.target }}" class="btn btn-green-big">
					{{ node.field_decouvrir_les_cours.title }}
                    <svg width="24" height="24" viewBox="0 0 24 24" fill="none"
                        xmlns="http://www.w3.org/2000/svg">
                        <path d="M14 16L18 12M18 12L14 8M18 12L6 12" stroke="white" stroke-width="1.5"
                            stroke-linecap="round" stroke-linejoin="round" />
                    </svg>
                </a>
            </div>
        </div>
    </div>
</div>
</section>
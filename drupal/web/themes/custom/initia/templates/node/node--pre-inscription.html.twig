										<div class="col-md-6">
											<strong>Sélectionnez une session</strong>
										</div>


										<div class="col-md-6">

											<div class="date-select">

												<span class="icon-date"><img src="images/calendar.svg"></span>
												<select class="form-control">


													<option value="" class="placeholder" disabled="" selected="selected">20/01/2025 au 31/01/2025</option>
													<option value="1">1</option>
													<option value="2">2</option>
													<option value="3">3</option>
												</select>
											</div>

										</div>
									</div>


								</div>


							</fieldset>


							<fieldset>

								<h3>Informations personnelles</h3>


								<div class="row">

									<div class="col-md-6">
										<div class="form-input">
											<lablel class="label-control">Prénom</lablel>
											<input type="text" class="form-control" placeholder="Prénom">
										</div>

									</div>


									<div class="col-md-6">
										<div class="form-input">
											<lablel class="label-control">Nom</lablel>
											<input type="text" class="form-control" placeholder="Nom">
										</div>

									</div>


								</div>

								<div class="row">
									<div class="col-md-12">

										<div class="form-input">
											<lablel class="label-control">E-mail</lablel>
											<input type="email" class="form-control" placeholder="E-mail">
										</div>

									</div>
								</div>


								<div class="row">
									<div class="col-md-3">

										<div class="form-input">
											<lablel class="label-control">Code</lablel>
											<input type="text" class="form-control" placeholder="FR  +33">
										</div>

									</div>

									<div class="col-md-9">
										<div class="form-input">
											<lablel class="label-control">Téléphone</lablel>
											<input type="tel" class="form-control" placeholder="369852255">
										</div>
									</div>
								</div>

							</fieldset>

							<fieldset>

								<h3>Adresse</h3>


								<div class="row">

									<div class="col-md-12">

<!--- main content-->
<main>


	<section class="contact pt-6 pb-6">


		<div class="container-fluid">


			<div class="row">
				<div class="col-md-6">
					<h2 class=" mt-4 mb-2 font-55">{{ node.label }}</h2>
					<p>{{ node.field_sub_title.value }}</p>
				</h2>
			</div>
		</div>
		<div class="row contact-content">
			<div class="col-md-6">


				<ul class="nav nav-tabs tabs-form mb-3 ms-0" id="myTab" role="tablist">
					<li class="nav-item" role="presentation">
						<button class="nav-link active" id="home-tab" data-bs-toggle="tab" data-bs-target="#particulier-tab-pane" type="button" role="tab" aria-controls="particulier-tab-pane" aria-selected="true">Particulier</button>
					</li>
					<li class="nav-item" role="presentation">
						<button class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#entreprise-tab-pane" type="button" role="tab" aria-controls="entreprise-tab-pane" aria-selected="false">Entreprise</button>
					</li>

				</ul>
				<div class="tab-content contact-form" id="myTabContent">
					<div class="tab-pane fade show active" id="particulier-tab-pane" role="tabpanel" aria-labelledby="particulier-tab" tabindex="0">

						{% if content.field_particulier_form %}
							{{ content.field_particulier_form }}
						{% endif %}
					</div>
					<div class="tab-pane fade" id="entreprise-tab-pane" role="tabpanel" aria-labelledby="entreprise-tab" tabindex="0">
						{% if content.field_entreprise_form %}
							{{ content.field_entreprise_form }}
						{% endif %}

					</div>

				</div>
			</div>

			{% if formation_card %}
				<div class="col-md-5 offset-lg-1">
				{{ formation_card }}
				</div>
			{% endif %}
				
		</div>


	</div>

</section></main><!-- main Content-->